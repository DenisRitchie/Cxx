{
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#1073cf2d",
        "editor.lineHighlightBorder": "#9fced11f",
        "activityBar.activeBackground": "#1a1a1a",
        "activityBar.background": "#1a1a1a",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#606020",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#1a1a1a",
        "statusBar.background": "#000000",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#1a1a1a",
        "statusBarItem.remoteBackground": "#000000",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#000000",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#00000099",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.color": "#000000",
    "editor.insertSpaces": true,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": false,
    "editor.tabSize": 2,
    "editor.useTabStops": true,
    "editor.tabCompletion": "on",
    "explorer.compactFolders": false,
    "git.ignoredRepositories": [
        "googletest-distribution",
        "googletest",
        "gmock",
        "gtest",
    ],
    "workbench.tree.indent": 20,
    "workbench.tree.expandMode": "doubleClick",
    "workbench.tree.renderIndentGuides": "always",
    "workbench.list.openMode": "doubleClick",
    "intellicodeCompletions.loggingLevel": "ALL",
    "clang-format.executable": "clang-format",
    "cmakeFormat.exePath": "",
    "cmake.generator": "Ninja",
    "cmake.useCMakePresets": "always",
    "cmake.languageSupport.enableFileAPI": true,
    "cmake.configureSettings": {
        "CMAKE_INSTALL_PREFIX": "${workspaceFolder}/Publish"
    },
    "testMate.cpp.test.executables": "{build,Build,BUILD,out,Out,OUT}/**/*{test,Test,TEST}*",
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "C_Cpp.default.cStandard": "c17",
    "C_Cpp.default.cppStandard": "c++23",
    "C_Cpp.errorSquiggles": "disabled",
    "C_Cpp.intelliSenseEngine": "default",
    "C_Cpp.intelliSenseCacheSize": 1048576,
    "C_Cpp.intelliSenseMemoryLimit": 16384,
    "C_Cpp.vcpkg.enabled": false,
    "files.encoding": "utf8",
    "files.insertFinalNewline": true,
    "files.maxMemoryForLargeFilesMB": 1048576,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "googletest.failed",
                "settings": {
                    "foreground": "#f00"
                }
            },
            {
                "scope": "googletest.passed",
                "settings": {
                    "foreground": "#0f0"
                }
            },
            {
                "scope": "googletest.run",
                "settings": {
                    "foreground": "#0f0"
                }
            }
        ]
    },
    "files.associations": {
        "*.php": "php",
        "*.rh": "cpp",
        "*.tcc": "cpp",
        "*.yml": "dockercompose",
        ".clang-tidy": "yaml",
